import math
m=10000001
seive=[True]*m
pf=[0]*m
seive[0]=seive[1]=False
for i in range(2,int(math.sqrt(m))+1):
    if seive[i]:
        for j in range(i*i,m,i):
            seive[j]=False
for i in range(m):
    if seive[i]:
        pf[i]=pf[i-1]+1
    else:
        pf[i]=pf[i-1]
n=int(input())
for i in range(n):
    a=int(input())
    print(pf[a])
    
input:
    3
    10
    5
    30
output:
    4
    3
    10
