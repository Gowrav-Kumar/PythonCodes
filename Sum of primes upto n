import math
m=10000
seive=[True]*m
pf=[0]*m
seive[0]=seive[1]=False
for i in range(2,int(math.sqrt(m))+1):
    if seive[i]:
        for j in range(i*i,m,i):
            seive[j]=False
m,n=map(int,input().split())
for i in range(1,n+1):
    if seive[i]:
        pf[i]=pf[i-1]+i
    else:
        pf[i]=pf[i-1]
print(pf[n]-pf[m-1])

input:  10 100
output: 1043
